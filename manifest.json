{
  "name": "Practice",
   "manifest_version":2,
    "version": "2.0",

    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },

    "content_scripts": [
      {
         "matches": ["http://*/*","https://*/*"],
  	      "js" :["insert_hello.js"]
     }
   ],

   "background": {
        "scripts": ["background.js"]
    },

    "permissions": [
        "contextMenus",
          "tabs",
          "background",
          "http://*/*",
          "https://*/*"
      ]

}
